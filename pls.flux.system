                pub {const} fn IPOPT_CLASS(o: u8) -> u8 { o & IPOPT_CLASS_MASK }

                pub {const} fn IPOPT_NUMBER(o: u8) -> u8 { o & IPOPT_NUMBER_MASK }

                pub {const} fn IPTOS_ECN(x: u8) -> u8 { x &  IPTOS_ECN_MASK }

