                                        fn eq(&self, other: &user_fpregs_struct) -> bool
                                        {
                                            self.cwd == other.cwd
                                            && self.swd == other.swd
                                            && self.ftw == other.ftw
                                            && self.fop == other.fop
                                            && self.rip == other.rip
                                            && self.rdp == other.rdp
                                            && self.mxcsr == other.mxcsr
                                            && self.mxcr_mask == other.mxcr_mask
                                            && self.st_space == other.st_space
                                            && self
                                            .xmm_space
                                            .iter()
                                            .zip(other.xmm_space.iter())
                                            .all(|(a, b)| a == b)
